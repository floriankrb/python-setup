
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all  --color-words --word-diff-regex=.
	lolb= log --graph --all --color-words --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	st = status
	s = status -s
	d = diff --color-words --word-diff-regex=.

[credential]
	helper = store

[user]
	email = firstname.lastname@emailprovider.fr
	name = Firstname Lastname


